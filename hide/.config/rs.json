{
    "name": "rust",
    "boxing": [
        {
            "colors": [7,8,9,1,2,3],
            "start": "[",
            "end": "]"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "{",
            "end": "}"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "(",
            "end": ")"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "<",
            "end": ">"
        }
    ],
    "custom": [
        {
            "color": 8,
            "capture": 2,
            "expr": "(^|\W)([A-Z][A-Z_]*)"
        },
        {
            "color": 0,
            "capture": 2,
            "expr": "(^|\W)([a-z_]*)"
        },
        {
            "color": 9,
            "capture": 2,
            "expr": "(^|\W)([A-Z][A-z][A-z]*)"
        },
        {
            "color": 2,
            "capture": 2,
            "expr": "(^|\W)([A-z_]*)\("
        },
        {
            "color": 1,
            "bold": true,
            "capture": 2,
            "expr": "(^|\W)(bool|int|short|double|float|long|wchar_t|char|string)($|\W)"
        },
        {
            "color": 9,
            "capture": 1,
            "bold": true,
            "expr": "([0-9][0-9.]*)"
        },
        {
            "color": 3,
            "bold": true,
            "capture": 2,
            "expr": "(^|\W)(crate|true|false|new|from|in|until|let|break|return|continue|case|default|catch|const|delete|this|extern|private|pub|public|protected|throw|try|register|typedef|friend|union|goto|unsigned|signed|virtual|void|volatile)($|\W)"
        },
        {
            "color": 8,
            "capture": 1,
            "expr": "(<|>|=|\+|-|\?|:|!|\/|\*|%|\||&|\^|~|,)"
        },
        {
            "color": 1,
            "capture": 2,
            "expr": "(^|\W)(if|else|else if|elif|match|select|switch|while|loop|for|foreach)($|\W)"
        },
        {
            "color": 1,
            "capture": 2,
            "expr": "(^|\W)(mod|use|class|struct|enum|mut|fn|;)($|\W)"
        },
        {
            "color": 3,
            "capture": 1,
            "expr": "\"(.*)\""
        },
        {
            "color": 9,
            "capture": 1,
            "expr": "('.*')"
        },
        {
            "color": 0,
            "capture": 1,
            "expr": "(//.*)"
        }
    ]
}
