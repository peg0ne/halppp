{
    "name": "halppp",
    "boxing": [
        {
            "colors": [7,8,9,1,2,3],
            "start": "[",
            "end": "]"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "{",
            "end": "}"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "(",
            "end": ")"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "<",
            "end": ">"
        }
    ],
    "custom": [
        {
            "color": 8,
            "capture": 2,
            "expr": "(^|\W)([A-Z][A-Z_]*)"
        },
        {
            "color": 0,
            "capture": 2,
            "expr": "(^|\W)([a-z_]*)"
        },
        {
            "color": 9,
            "capture": 2,
            "expr": "(^|\W)([A-Z][A-z][A-z]*)"
        },
        {
            "color": 2,
            "capture": 2,
            "expr": "(^|\W)([A-z_]*)\("
        },
        {
            "color": 1,
            "bold": true,
            "capture": 1,
            "expr": "(?:^|\W)(bool|int|short|double|float|long|char|string)(?=$|\W)"
        },
        {
            "color": 9,
            "capture": 1,
            "bold": true,
            "expr": "([0-9][0-9.]*)"
        },
        {
            "color": 9,
            "capture": 1,
            "bold": true,
            "expr": "(?:^|\W)(True|False|null)(?=$|\W)"
        },
        {
            "color": 3,
            "bold": true,
            "capture": 1,
            "expr": "(?:^|\W)(new|from|and|not|in|until|break|return|continue|case|default|except|catch|const|delete|this|throw|try|register|void|)(?=$|\W)"
        },
        {
            "color": 8,
            "capture": 1,
            "expr": "(<|>|=|\+|-|\?|:|!|\/|\*|%|\||&|\^|~|,|\$)"
        },
        {
            "color": 1,
            "capture": 1,
            "expr": "(?:^|\W)(if|else|elif|select|switch|while|loop|for|foreach)(?=$|\W)"
        },
        {
            "color": 1,
            "capture": 1,
            "expr": "(?:^|\W)(global|import|class|struct|enum|def|async|;)(?=$|\W)"

        },
        {
            "color": 3,
            "capture": 1,
            "expr": "\"([^\"]*)\""
        },
        {
            "color": 9,
            "capture": 1,
            "expr": "('[^']')"
        },
        {
            "color": 0,
            "capture": 1,
            "expr": "(#.*)"
        }
    ],
    "wordlist": [
        "False",
        "True",
        "None",
        "from",
        "import",
        "if:",
        "elif:",
        "else:",
        "async",
        "def",
        "return",
        "global",
        "startswith",
        "split",
        "append"
    ]
}