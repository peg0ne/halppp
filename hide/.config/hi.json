{
    "name": "hi",
    "boxing": [
        {
            "colors": [7,8,9,1,2,3],
            "start": "[",
            "end": "]"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "{",
            "end": "}"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "(",
            "end": ")"
        },
        {
            "colors": [7,8,9,1,2,3],
            "start": "<",
            "end": ">"
        }
    ],
    "custom": [
        {
            "color": 9,
            "capture": 2,
            "expr": "(^|\W)([A-Z][A-z][A-z]*)"
        },
        {
            "color": 8,
            "capture": 2,
            "bold": true,
            "expr": "(^|\W)([A-Z][A-Z_]*)"
        },
        {
            "color": 0,
            "capture": 2,
            "expr": "(^|\W)([a-z_]*)"
        },
        {
            "color": 2,
            "capture": 2,
            "expr": "(^|\W)([A-z_]*)\("
        },
        {
            "color": 2,
            "capture": 2,
            "expr": "(cfn|fn) ([A-z_]*)\W"
        },
        {
            "color": 9,
            "capture": 1,
            "bold": true,
            "expr": "([0-9][0-9.]*)"
        },
        {
            "color": 8,
            "capture": 1,
            "expr": "(<|>|=|\+|-|\?|:|!|\/|\*|%|\||&|\^|~| at | len| is | or | and |,|\$)"
        },
        {
            "color": 2,
            "capture": 1,
            "expr": "(?:^|\W)(if|let|loop|for)(?=$|\W)"
        },
        {
            "color": 2,
            "capture": 1,
            "bold": true,
            "expr": "(?:^|\W)(random|tonum|filer|import|filew|sys|eval)(?=$|\W)"
        },
        {
            "color": 1,
            "capture": 1,
            "bold": true,
            "expr": "(?:^|\W)(cfn|fn|break|return|exit|read|print|printnobr|argc|argv)(?=$|\W)"
        },
        {
            "color": 3,
            "capture": 1,
            "expr": "\"([^\"]*)\""
        },
        {
            "color": 9,
            "capture": 1,
            "expr": "('[^']')"
        },
        {
            "color": 0,
            "capture": 1,
            "dim": true,
            "expr": "(//.*)"
        }
    ],
    "wordlist": [
        "if",
        "print",
        "exit",
        "eval",
        "let",
        "printnobr",
        "RED",
        "REDBG",
        "GREEN",
        "GREENBG",
        "BLUE",
        "BLUEBG",
        "YELLOW",
        "YELLOWBG",
        "MAGENTA",
        "MAGENTABG",
        "CYAN",
        "CYANBG",
        "read",
        "RESET",
        "filer",
        "filew",
        "tonum"
    ]
}
